                    <div class="widget">
                        <div class="input-group mb-3 border rounded">
                            <input type="text" id="s" name="s" class="form-control border-0" placeholder="Nhập từ khóa...">
                        </div>
                       
                    </div>
                    <!-- SEARCH -->
                    <!-- Categories -->
                    <div class="widget mb-4 pb-4 border-bottom">
                        <h5 class="widget-title">Danh mục</h5>
                        <div class="mt-4 mb-0">
                            <select name="c" class="form-select form-control" aria-label="Default select example">
                                <option selected="" value="all">Tất cả</option>
                                @foreach (var obj in Model)
                                {
                                    <option value="@obj.Slug">@obj.Name</option>
                                }
                            </select>
                        </div>
                    </div>
                    <!-- Categories -->
                    <!-- Posted -->
                    <div class="widget mb-4 pb-4 border-bottom">
                        <h5 class="widget-title">Ngày đăng</h5>
                        <div class="mt-4">
                            <div class="form-check mb-0">
                                <input class="form-check-input" checked="" type="radio" name="postedAt" value="today" id="lasthour">
                                <label class="form-check-label" for="lasthour">Hôm nay</label>
                            </div>

                            <div class="form-check mb-0">
                                <input class="form-check-input" checked="" type="radio" name="postedAt" value="yesterday" id="oneday">
                                <label class="form-check-label" for="oneday">Hôm qua</label>
                            </div>

                            <div class="form-check mb-0">
                                <input class="form-check-input" checked="" type="radio" name="postedAt" value="lastweek" id="sevenday">
                                <label class="form-check-label" for="sevenday">Tuần trước</label>
                            </div>

                            <div class="form-check mb-0">
                                <input class="form-check-input" checked="" type="radio" name="postedAt" value="lastmonth" id="onemonth">
                                <label class="form-check-label" for="onemonth">Tháng trước</label>
                            </div>
                        </div>
                    </div>
                    <button type="submit" class="btn btn-primary">Lọc</button>
                    <!-- Posted -->
                    </form>
